{
    "$schema": "https://schema.railpack.com",
    "steps": {
        "install": {
            "commands" : ["..."]
        },
        "build" : {
            "commands": [
                "php bin/console cache:clear --env=$APP_ENV --no-warmup",
                "php bin/console cache:warmup --env=$APP_ENV",
                "php bin/console doctrine:migrations:migrate --no-interaction --env=$APP_ENV || true"
            ]
        },
        "deploy" : {
            "startCommand" : "php -S 0.0.0.0:8000 -t public",
            "paths": [],
            "base": null
        }
    }
}